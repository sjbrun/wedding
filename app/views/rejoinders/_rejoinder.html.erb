<%#= paginate obj %>

<table class="table">
  <thead>
    <tr>
      <th>YES</th>
      <th>NO</th>
      <th>Guest(s)</th>
      <th>#</th>
      <th>Message</th>
    </tr>
  </thead>
  <tbody>
    <% obj.each do |rsvp| %>
      <tr>
        <td><%= 'X' if rsvp.yes %></td>
        <td><%= 'X' if !rsvp.yes %></td>
        <td><%= rsvp.names %></td>
        <td><%= rsvp.number %></td>
        <td><%= rsvp.msg if rsvp.msg %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%#= paginate obj %>
